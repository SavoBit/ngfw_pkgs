<div class="list-table redirects">
  <ul id="redirect-list">
    <li class="header">
      <div class="enabled"><%= "Enabled".t %></div>
      <div class="description"><%= "Description".t %></div>
      <div class="edit"></div>
      <div class="minus"></div>
