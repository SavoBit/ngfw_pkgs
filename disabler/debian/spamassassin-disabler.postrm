#!/bin/sh

# leave used flag 
mkdir -p /usr/share/metavize/conf/trials &> /dev/null
touch /usr/share/metavize/conf/trials/spamassassin-used
# reenable usage
mcli enable spamassassin-transform &> /dev/null
# change extraname to nothing
mcli extraname spamassassin-transform "" &> /dev/null &

exit 0


