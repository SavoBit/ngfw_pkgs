#!/bin/sh

# leave used flag 
mkdir -p /usr/share/metavize/conf/trials &> /dev/null
touch /usr/share/metavize/conf/trials/protofilter.expired

# reenable usage
mcli enable protofilter-transform &> /dev/null
# change extraname to nothing
mcli extraname protofilter-transform "" &> /dev/null &

exit 0


