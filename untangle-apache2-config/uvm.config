NameVirtualHost *:80
NameVirtualHost *:443
NameVirtualHost *:64157

<VirtualHost *:80>
    DocumentRoot /var/www/

    Include /etc/apache2/untangle-unrestricted
    Include /etc/apache2/untangle-internal
    Include /etc/apache2/untangle-homepage

    ErrorLog /var/log/apache2/error.log
    CustomLog /var/log/apache2/access.log combined
</VirtualHost>

Alias /auth /usr/share/untangle/mod_python/auth

<Directory /usr/share/untangle/mod_python/auth>
    SetHandler mod_python
    PythonHandler mod_python.publisher
</Directory>

<VirtualHost *:443>
    DocumentRoot /var/www/

    Include /etc/apache2/untangle-unrestricted
    Include /etc/apache2/untangle-internal
    Include /etc/apache2/untangle-homepage

    ErrorLog /var/log/apache2/error-ssl.log
    CustomLog /var/log/apache2/access-ssl.log combined

    SSLEngine on
    SSLCertificateFile /etc/apache2/ssl/apache.pem
</VirtualHost>

<VirtualHost *:64157>
    DocumentRoot /var/www/

    Include /etc/apache2/untangle-unrestricted
    Include /etc/apache2/untangle-internal
    Include /etc/apache2/untangle-homepage

    ErrorLog /var/log/apache2/error-ssl.log
    CustomLog /var/log/apache2/access-ssl.log combined

    SSLEngine on
    SSLCertificateFile /etc/apache2/ssl/apache.pem
</VirtualHost>
