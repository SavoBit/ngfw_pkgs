#!/bin/bash
if [ ${#UVM_ROOT} != 0 ] ; then
    NUCLI_ROOT=${UVM_ROOT}/../../pkgs/untangle-cli-server/files
else
    NUCLI_ROOT=""
fi

export RUBYLIB=${UVM_ROOT}/usr/share/untangle/nucli:$RUBYLIB

/usr/bin/uvm-jruby -X ${NUCLI_ROOT}/usr/share/untangle-cli-server/src ./server.rb $1 $2 $3 $4 $5 $6 $7 $8 $9 &
