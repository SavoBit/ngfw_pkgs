#!/bin/dash

## This doesn't actually do anything.
CPD_HOME=${CPD_HOME:-""}

## This script will clear all of the empty IPSets which may accumulate over time.
${CPD_HOME}/usr/share/untangle-cpd/bin/refresh_iptables.lua

for t_table in `${CPD_HOME}/usr/share/untangle-cpd/bin/get_ipsets empty` ; do
    ipset -X ${t_table}
done




