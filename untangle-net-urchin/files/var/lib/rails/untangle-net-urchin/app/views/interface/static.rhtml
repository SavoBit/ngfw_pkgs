<div>
  <h1>Static interface configuration for <%= @interface.name %></h1>
  
  <table>
    <tr>
      <th>Address and Netmask</th>
      <th>Delete</th>
    </tr>
    
    <% for network in @staticConfig.ip_networks %>
    <tr>
      <td><%= "#{network.ip} / #{network.netmask}" %></td>
      <td>delete</td>
    </tr>
    <% end %>
  </table>
</div>