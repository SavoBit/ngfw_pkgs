#!/bin/sh
#
# Remap the interfaces to the way they should be
#

IFRENAME=/sbin/ifrename

case "$1" in
  start)
    if [ -x ${IFRENAME} ]; then
      ${IFRENAME} -t
    else    
      echo "${IFRENAME} is not installed, unable to remap interfaces."
    fi
    ;;
  stop)
    ;;
  restart|force-reload)
    ;;
  *)  
    echo "Usage: $0 {start|stop|restart|force-reload}" >&2
    exit 1
    ;;
esac
